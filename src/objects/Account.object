    <validationRules>
        <fullName>Account_Number_8_Characters</fullName>
        <active>false</active>
        <errorConditionFormula>LEN( AccountNumber) != 8</errorConditionFormula>
        <errorMessage>Account number must be 8 characters long.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_Number_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>(!ISBLANK(AccountNumber)) &amp;&amp;  ISNUMBER(AccountNumber)</errorConditionFormula>
        <errorMessage>Account number should be numeric</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Account_Number_Validation_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(Credit_Card_Number__c),NOT( REGEX( Credit_Card_Number__c , &quot;(((\\d{4}-){3}\\d{4})|\\d{16})?&quot;)))</errorConditionFormula>
        <errorMessage>Credit Card number not valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_Postal_Code_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(BillingCountry = &quot;USA&quot;, BillingCountry =
&quot;US&quot;),
NOT(REGEX(BillingPostalCode, &quot;\\d{5}(-\\d{4})?&quot;))
)</errorConditionFormula>
        <errorMessage>Billing postal code not valid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mic_Pct_Validation_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
   Mix_Pct__c &gt; 1.0,
   Mix_Pct__c &lt; 0.0
)</errorConditionFormula>
        <errorMessage>The percentage should be between 0 and 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Multiple_Of_Five_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>MOD(  Multiple_of_Five__c , 5) &lt;&gt; 0</errorConditionFormula>
        <errorMessage>Number should be a multiple of five</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Weekday_Validation_Rules</fullName>
        <active>true</active>
        <errorConditionFormula>CASE(MOD( Weekday__c - DATE(1900, 1, 7), 7),
0, 0,
6, 0,
1) = 0</errorConditionFormula>
        <errorMessage>Date must be a weekday</errorMessage>
    </validationRules>
