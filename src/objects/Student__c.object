        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Edit Name</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/apex.js&quot;)} 
queryResult = sforce.connection.query(&quot;Select ID, Name From Student__c where id=&apos;{!Student__c.Id}&apos;&quot;); 
var records = queryResult.getArray(&quot;records&quot;); 
window.open(&apos;/apex/EditStudentName?id=&apos; + records[0].Id, &quot;&quot;, &quot;width=700,height=150&quot;);</url>
