    <webLinks>
        <fullName>Delete_Students</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Delete Students</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/apex.js&quot;)}
var records = {!GETRECORDIDS($ObjectType.Student__c)};
window.open(&apos;/apex/DeleteStudents?id=&apos;+records);</url>
    </webLinks>
    <webLinks>
        <fullName>Edit_Name</fullName>
